<script type="text/javascript">



        function doAnimation()
        {
                $('#count1').each(function () {
                    $(this).prop('Counter',0).animate({
                    Counter: $(this).text()
                    }, {
                    duration: 4000,
                    easing: 'swing',
                    step: function (now) {
                        $(this).text(Math.ceil(now));
                    }
                    });
                });
                $('#count2').each(function () {
